/*--------- print group ----------------- */

/*       integer*2  LPTLFN, PRFLFN, SPLLFN */
#define	LPTLFN	21	 /*                           - 1st printer lfn */
#define	PRFLFN	22	 /*                           - 1st print file lfn */
#define	SPLLFN	23	 /*                           - 1st spool file lfn */

/*     Number of lfn's = (# of printers)*3 + 20 */
/*                     =         15(max)*3 + 20 */
/*                     =         65 */

/*--------------------------------------- */

/*     Allow for expansion. */
/*     Allow for floating/OS assigned lfn's. */

/*       integer*2  NUM_LFNS */
#define	NUM_LFNS	350	 /*   Therefore give groups 250 lfn's */

