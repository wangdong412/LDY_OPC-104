
/*     ******* SCADA LFN NUMBERS ****** */

/*     SCCS REVISION: */
/*	  @(#)lfns,v	830.1 */

/*     DESCRIPTION: */
/*         Files are shared between groups and tasks. Hence a set of global */
/*         lfn's have been assigned. The one exception is the print group */
/*         which only accesses DATA, EVENTS, DELAYS,SOES, and printers. */
/*         The printer lfn's are not unique. */

/*       integer*2  RUNLFN , BUPLFN , TAGLFN , EVTLFN , DLYLFN */
/*       integer*2  SOELFN , BKRLFN , DFDLFN , SHALFN , RPTLFN */
/*       integer*2  HXRLFN , HRLLFN , HWILFN , HCILFN */
/*       integer*2  DDTLFN , PADLFN , SPADLFN, SDDTLFN */
/*       integer*2  DSPTLFN, SHATLFN, OIDLFN */
/*       integer*2  H00LFN , H01LFN , H02LFN , H03LFN , H04LFN */
/*       integer*2  H05LFN , H06LFN , H07LFN , H08LFN , H09LFN */
/*       integer*2  H10LFN , H11LFN , H12LFN , H13LFN , H14LFN , H15LFN */

/*       integer*2  AMPLFN */
/*       integer*2  ATPDLFN, AZONLFN, ALVLLFN, ARPTLFN, AAGRLFN */
/*       integer*2  ACRDLFN, ADVALFN, ASYSLFN, AIMGLFN */
/*       integer*2  AGRPLFN, ACDMLFN, ACODLFN, AKEYLFN */
/*       integer*2  ARMPLFN */
/*       integer*2  CAMCLFN, PSTSLFN, SHFTLFN, LGHTLFN, AFGPLFN */
/*       integer*2  RCPLFN , SPPRECLFN, UMAXSPLFN */
/*       integer*2  DMTLFN , TOURLFN, IVRLFN , MASCLFN */
/*       integer*2  GTSSLFN, GTSGLFN, GTSCLFN, GTSNLFN */
/*       integer*2  XL10ZMLFN, XL10XLLFN, XL10SLLFN */
/*       integer*2  SECHIPLFN, ACSACTLFN, EVTSEQLFN */
/*       integer*2  GRPCTRLLFN, U800SPLFN, U800ADLFN */

/*       integer*2  U01LFN , U02LFN , U03LFN , U04LFN , U05LFN */
/*       integer*2  U06LFN , U07LFN , U08LFN , U09LFN , U10LFN */
/*       integer*2  U11LFN , U12LFN , U13LFN , U14LFN , U15LFN */
/*       integer*2  U16LFN , U17LFN , U18LFN , U19LFN , U20LFN */
/*       integer*2  U21LFN , U22LFN , U23LFN , U24LFN , U25LFN */
/*       integer*2  U26LFN , U27LFN , U28LFN , U29LFN , U30LFN */
/*       integer*2  U31LFN , U32LFN , U33LFN , U34LFN , U35LFN */
/*       integer*2  U36LFN , U37LFN , U38LFN , U39LFN , U40LFN */
/*       integer*2  U41LFN , U42LFN , U43LFN , U44LFN , U45LFN */
/*       integer*2  U46LFN , U47LFN , U48LFN , U49LFN , U50LFN */
/*       integer*2  U51LFN , U52LFN , U53LFN , U54LFN , U55LFN */
/*       integer*2  U56LFN , U57LFN , U58LFN , U59LFN , U60LFN */
/*       integer*2  U61LFN , U62LFN , U63LFN , U64LFN , U65LFN */
/*       integer*2  U66LFN , U67LFN , U68LFN , U69LFN , U70LFN */
/*       integer*2  U71LFN , U72LFN , U73LFN , U74LFN , U75LFN */
/*       integer*2  U76LFN , U77LFN , U78LFN , U79LFN , U80LFN */
/*       integer*2  U81LFN , U82LFN , U83LFN , U84LFN , U85LFN */
/*       integer*2  U86LFN , U87LFN , U88LFN , U89LFN , U90LFN */
/*       integer*2  U91LFN , U92LFN , U93LFN , U94LFN , U95LFN */
/*       integer*2  U96LFN , U97LFN , U98LFN , U99LFN , U100LFN */
/*       integer*2  U101LFN, U102LFN, U103LFN, U104LFN, U105LFN */
/*       integer*2  U106LFN, U107LFN, U108LFN, U109LFN, U110LFN */
/*       integer*2  U111LFN, U112LFN, U113LFN, U114LFN, U115LFN */
/*       integer*2  U116LFN, U117LFN, U118LFN, U119LFN, U120LFN */
/*       integer*2  U121LFN, U122LFN, U123LFN, U124LFN, U125LFN */
/*       integer*2  U126LFN, U127LFN, U128LFN, U129LFN, U130LFN */
/*       integer*2  U131LFN, U132LFN, U133LFN, U134LFN, U135LFN */
/*       integer*2  U136LFN, U137LFN, U138LFN, U139LFN, U140LFN */
/*       integer*2  U141LFN, U142LFN, U143LFN, U144LFN, U145LFN */
/*       integer*2  U146LFN, U147LFN, U148LFN, U149LFN, U150LFN */

/*                                file pathname stored in LFNTBL (24 char) */
/*                                123456789012345678901234 */
#define	RUNLFN	7	 /*  <DATA>DATA               - main data file */
#define	BUPLFN	8	 /*  <DATA>DATA               - backup data file */
#define	TAGLFN	9	 /*  <DATA>TAGFL              - tag file */
#define	EVTLFN	10	 /*  <DATA>EVENTS             - event file */
#define	DLYLFN	11	 /*  <DATA>DELAYS             - delay file */
#define	SOELFN	12	 /*  <DATA>SOES               - SOE file */
#define	BKRLFN	13	 /*  <DATA>CRTBKR             - display backgrounds */
#define	DFDLFN	14	 /*  <DATA>CRTDFD             - display DFD's */
#define	SHALFN	15	 /*  <DATA>CRTSHA             - shapes */
#define	RPTLFN	16	 /*                           - report lfn */
/*     parameter (       = 17) ;* */
#define	HXRLFN	18	 /*  <DATA>HXR000             - hardware xreference */
#define	HRLLFN	19	 /*  <DATA>HRL000             - hardware ref. list */
#define	HWILFN	20	 /*  <DATA>HWI000             - hardware image */
#define	HCILFN	21	 /*  <DATA>HCI000             - hard. config. image */
#define	DDTLFN	22	 /*  <DATA>LCNDDT             - LCN ddt file */
#define	PADLFN	23	 /*  <DATA>LCNPAD             - LCN PAD file */
#define	SPADLFN	24	 /*  <DATA>SCNPAD             - pad file */
#define	SDDTLFN	25	 /*  <DATA>SCNDDT             - ddt file */
#define	DSPTLFN	26	 /*  <DATA>DSPTBL             - display titles */
#define	SHATLFN	27	 /*  <DATA>SHATBL             - shape titles */
#define	OIDLFN	28	 /*  <DATA>OPERID             - operator profiles */
/*     parameter (       = 29) ;* */
/*     parameter (       = 30) ;* */
/*     parameter (       = 31) ;* */
/*     parameter (       = 32) ;* */
/*     parameter (       = 33) ;* */
/*     parameter (       = 34) ;* */
#define	H00LFN	35	 /*  <DATA>HISTORY0           - history 0 */
#define	H01LFN	36	 /*  <DATA>HISTORY1           - history 1 */
#define	H02LFN	37	 /*  <DATA>HISTORY2           - history 2 */
#define	H03LFN	38	 /*  <DATA>HISTORY3           - history 3 */
#define	H04LFN	39	 /*  <DATA>HISTORY4           - history 4 */
#define	H05LFN	40	 /*  <DATA>HISTORY5           - history 5 */
#define	H06LFN	41	 /*  <DATA>HISTORY6           - history 6 */
#define	H07LFN	42	 /*  <DATA>HISTORY7           - history 7 */
#define	H08LFN	43	 /*  <DATA>HISTORY8           - history 8 */
#define	H09LFN	44	 /*  <DATA>HISTORY9           - history 9 */
#define	H10LFN	45	 /*  <DATA>HISTORY10          - history 10 */
#define	H11LFN	46	 /*  <DATA>HISTORY11          - history 11 */
#define	H12LFN	47	 /*  <DATA>HISTORY12          - history 12 */
#define	H13LFN	48	 /*  <DATA>HISTORY13          - history 13 */
#define	H14LFN	49	 /*  <DATA>HISTORY14          - history 14 */
#define	H15LFN	50	 /*  <DATA>HISTORY15          - history 15 */

/* leave a gap here for dynamic LFN allocation as FORTRAN can only */
/* use unit numbers up to 99. */

#define	AMPLFN	100	 /*  <DATA>AMPTBL             - Asset Management Permissions */
/*     parameter (       =101) ;* */
#define	ATPDLFN	102	 /*  <DATA>ACSTPD             - Access Time Periods */
#define	AZONLFN	103	 /*  <DATA>ACSZON             - Access Zones */
#define	ALVLLFN	104	 /*  <DATA>ACSLVL             - Access Levels */
