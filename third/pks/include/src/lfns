#define	AAGRLFN	106	 /*  <DATA>ACSAGR             - Access Alarm Groups */
#define	ACRDLFN	107	 /*  <DATA>ACSCRD             - Access Cards */
#define	ADVALFN	108	 /*  <DATA>ACSDVA             - Access Dev. Assign. */
#define	ASYSLFN	109	 /*  <DATA>ACSSYS             - Access SE System */
#define	AIMGLFN	110	 /*  <DATA>ACSIMG             - Access SE Image */
#define	AGRPLFN	111	 /*  <DATA>ACSGRP             - Access Groups */
#define	ACDMLFN	112	 /*  <DATA>ACSCDM             - Access Codes Map */
#define	ACODLFN	113	 /*  <DATA>ACSCOD             - Access Codes */
#define	AKEYLFN	114	 /*  <DATA>ACSKEY             - Access Keys */
/*     parameter (       =115) ;* */
/*     parameter (       =117) ;* */
#define	ARMPLFN	119	 /*  <DATA>GDAMAP             - opc area mappings */
#define	CAMCLFN	120	 /*  <DATA>CAMCFG             - cam/mon assignments */
#define	PSTSLFN	121	 /*  <DATA>PNTSTS             - Point sts summary */
#define	SHFTLFN	122	 /*  <DATA>SHFMGR             - Shift manager */
#define	LGHTLFN	123	 /*  <DATA>LIGHTS             - Lighting Control */
#define	AFGPLFN	124	 /*  <DATA>ACSFGP             - uELV Floor Goups */
#define	RCPLFN	125	 /*  <DATA>RECIPES            - recipe table */
